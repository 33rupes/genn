      "import scipy.stats as stat\n",
      "import numpy as np\n",
      "import sys\n",
      "\n",
      "bins = np.linspace(-2.0, 2.0, 100 + 1)\n",
      "\n",
      "cdf = [stat.norm.cdf(x) for x in bins]\n",
      "\n",
      "for c_start, c_stop in zip(cdf[:-1], cdf[1:]):\n",
      "    sys.stdout.write(\"%f, \" % ((c_stop - c_start)))"
